extends layout

block content
  
  .w3-container.w3-margin.w3-light-gray
    h3 Indivíduo #{individual} da Classe #{classe} do repositório #{repo} 
  .w3-container.w3-border
    h3 Lista de Datap Datao
    .w3-container.w3-margin
      div.w3-row.w3-margin
        div.w3-col.l3
          table.w3-table-all
            each dp in datap
              tr
                td
                  a= dp
        div.w3-col.l3
          table.w3-table-all
            each dob in datao
              if(dob != undefined)
                tr
                  td
                    a= dob
              else 
                tr
                  td
                    a -

  footer
    .w3-container.w3-light-gray 
      .w3-container.w3-margin.w3-border
        a(href="/"+repo+"/enteties/"+classe) Voltar
        

    